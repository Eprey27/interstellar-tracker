# Terraform Variables for Local Development
# Copy this file to terraform/environments/dev/terraform.tfvars
# NEVER commit terraform.tfvars to version control!

# =============================================================================
# REQUIRED: Update these values with your information
# =============================================================================

# Your Azure Subscription ID
subscription_id = "9fff6f2c-c722-4906-bb36-19bcd059d6d6"

# Your email for resource ownership and alerts
owner_email = "YOUR_EMAIL@example.com"
alert_email = "YOUR_EMAIL@example.com"

# =============================================================================
# OPTIONAL: Uncomment and customize if needed
# =============================================================================

# If the default Key Vault name is taken (globally unique)
# key_vault_name = "interstellar-dev-kv-yourname"

# Add your public IP to allow Key Vault access
# Get your IP: curl ifconfig.me
# key_vault_allowed_ips = ["YOUR.PUBLIC.IP.ADDRESS"]

# Change Azure region (default: West Europe)
# location = "North Europe"

# Custom resource names
# resource_group_name          = "my-custom-rg"
# log_analytics_workspace_name = "my-custom-law"
# application_insights_name    = "my-custom-ai"
